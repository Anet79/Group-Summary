<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Fab_Bottom_app_bar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorForBackground</item>
        <item name="colorPrimaryVariant">@color/colorForBackground02</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorForBackground01</item>
        <item name="colorSecondaryVariant">@color/colorForBackground01</item>
        <item name="colorOnSecondary">@color/colorForBackground01</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="colorPrimaryDark">@color/colorForBackground01</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.FinalProject" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/colorForBackground</item>
        <item name="colorPrimaryVariant">@color/colorForBackground01</item>
        <item name="colorOnPrimary">@color/colorForBackground01</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="colorPrimaryDark">@color/colorForBackground01</item>
    </style>




    <style name="Theme.FinalProject.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.FinalProject.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FinalProject.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>