<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Fab_Bottom_app_bar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#9F8772</item>
        <item name="colorPrimaryVariant">#D7CCC8</item>
        <item name="colorOnPrimary">#D7CCC8</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/bar2</item>
        <item name="colorSecondaryVariant">@color/bar2</item>
        <item name="colorOnSecondary">#9F8772</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <item name="colorPrimaryDark">#9F8772</item>
        <!-- Customize your theme here. -->
    </style>

    <!--  Specific Rounded Corner-->
    <style name="specific_rounded_corner">
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
    </style>

    <!-- Circle ImageView-->
    <style name="circle">
        <item name="cornerSize">50%</item>
    </style>
    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>


